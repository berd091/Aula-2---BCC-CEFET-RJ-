#include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    float altura, peso, imc;
    
    printf("Digite sua altura em metros(m): ");
    scanf("%f", &altura);
    
    printf("\nDigite seu peso em quilos(kg): ");
    scanf("%f", &peso);
    
    imc = peso / (altura * altura);
    printf("\nSeu IMC e: %f", imc);

    return 0;
}
